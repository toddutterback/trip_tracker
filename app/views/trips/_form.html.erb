<%= form_for @trip do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br />
  <%= f.label :start_date %>
  <%= f.text_field :start_date %>
  <br />
  <%= f.label :end_date %>
  <%= f.text_field :end_date %>
  <br />
  <%= f.label :description%>
  <%= f.text_field :description %>
  <br />
  <%= f.submit %>
<% end %>